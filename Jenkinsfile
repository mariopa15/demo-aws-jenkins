pipeline {    agent any    environment {        AWS_DEFAULT_REGION = "us-east-1"        BUCKET_NAME = "pruebajenkis--use1-az4--x-s3"    }    stages {        stage('Checkout') {            steps {                git branch: 'main',                    url: 'https://github.com/mariopa15/demo-aws-jenkins.git'            }        }        stage('Deploy to S3') {            steps {                withAWS(region: "${AWS_DEFAULT_REGION}", credentials: 'aws-credentials') {                    sh """                        echo 'ðŸ“¦ Subiendo index.html a S3...'                        aws s3 cp index.html s3://${BUCKET_NAME}/index.html --acl public-read                    """                }            }        }    }}