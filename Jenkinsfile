pipeline {    agent any    environment {        BUCKET_NAME = 'mi-bucket-demo-jenkins'        AWS_REGION  = 'us-east-1'    }    stages {        stage('Checkout') {            steps {                git branch: 'main', url: 'https://github.com/USUARIO/REPO.git'            }        }        stage('Deploy to S3') {            steps {                sh '''                # Crear bucket si no existe                aws s3 mb s3://$BUCKET_NAME --region $AWS_REGION || echo "Bucket ya existe"                # Subir archivo                aws s3 cp index.html s3://$BUCKET_NAME/index.html --region $AWS_REGION                echo "Despliegue completado"                '''            }        }    }}