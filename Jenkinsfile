pipeline {    agent any    environment {        AWS_REGION = 'us-east-1'        BUCKET_NAME = 'mi-bucket-unico-jenkins-2025' // Cambia por un nombre √∫nico    }    stages {        stage('Crear S3 Bucket') {            steps {                echo "‚òÅÔ∏è Creando bucket S3 con Terraform"                sh '''                    # Crear archivo Terraform                    cat > s3.tf <<EOLterraform {  required_providers {    aws = {      source  = "hashicorp/aws"      version = "~> 5.0"    }  }}provider "aws" {  region = "${AWS_REGION}"}resource "aws_s3_bucket" "mi_bucket" {  bucket = "${BUCKET_NAME}"  acl    = "private"}EOL                    # Inicializar Terraform                    terraform init -input=false -no-color                    # Aplicar configuraci√≥n autom√°ticamente                    terraform apply -input=false -auto-approve                '''            }        }    }    post {        always {            echo "üìú Pipeline finalizado. Revisa los logs."        }        success {            echo "‚úÖ Bucket creado correctamente"        }        failure {            echo "‚ùå Fall√≥ la creaci√≥n del bucket. Revisa errores anteriores"        }    }}